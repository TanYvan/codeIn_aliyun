        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="caseDocIn" extends="common" namespace="/caseDocIn">
		<action name="list" class="CaseDocInAction" method="list">
			<result name="success">/WEB-INF/pages/caseDocIn/list.jsp</result>
